    "clean-run-build": "npm run migrate-build && cd ../backend/Scripts && activate && cd .. && python manage.py runserver",
    "run-build": " cd ../backend/Scripts && activate && cd .. && python manage.py runserver"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {}
